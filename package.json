{
  "name": "safe-airdrop",
  "version": "1.2.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --watchAll=false --verbose",
    "lint": "eslint --max-warnings 0 .",
    "coverage": "yarn test --coverage",
    "eject": "react-scripts eject",
    "fmt": "prettier --check '**/*.ts'",
    "fmt:write": "prettier --write '**/*.ts'",
    "prepare": "husky install",
    "generate-types": "typechain --target=ethers-v5 --out-dir src/contracts './node_modules/@openzeppelin/contracts/build/contracts/ERC20.json' './customabis/ERC721.json' './node_modules/@openzeppelin/contracts/build/contracts/ERC1155.json' './node_modules/@openzeppelin/contracts/build/contracts/ERC165.json'",
    "postinstall": "yarn generate-types"
  },
  "dependencies": {
    "@fast-csv/parse": "^4.3.6",
    "@gnosis.pm/safe-apps-provider": "^0.11.0",
    "@gnosis.pm/safe-apps-react-sdk": "^4.3.1",
    "@gnosis.pm/safe-apps-sdk": "^7.3.0",
    "@gnosis.pm/safe-react-components": "^1.1.2",
    "@material-ui/core": "^4.12.3",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@openzeppelin/contracts": "^4.5.0",
    "ace-builds": "^1.4.14",
    "bignumber.js": "^9.0.2",
    "ethers": "^5.6.2",
    "honeyswap-default-token-list": "^3.1.1",
    "lodash.debounce": "^4.0.8",
    "react": "^17.0.2",
    "react-ace": "^10.1.0",
    "react-app-rewired": "^2.2.1",
    "react-csv-reader": "^3.4.0",
    "react-dom": "^17.0.2",
    "react-dropzone": "^13.0.0",
    "react-scripts": "^4.0.3",
    "react-svg": "^15.0.0",
    "react-virtualized-auto-sizer": "^1.0.6",
    "react-window": "^1.8.6",
    "styled-components": "^5.3.3",
    "tslib": "^2.4.0",
    "typescript": "~4.4.4"
  },
  "devDependencies": {
    "@simbathesailor/use-what-changed": "^2.0.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.3",
    "@typechain/ethers-v5": "^7.1.2",
    "@types/chai": "^4.3.0",
    "@types/chai-as-promised": "^7.1.5",
    "@types/lodash.debounce": "^4.0.7",
    "@types/node": "^17.0.38",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^18.0.3",
    "@types/react-virtualized-auto-sizer": "^1.0.1",
    "@types/react-window": "^1.8.5",
    "@types/styled-components": "^5.1.23",
    "babel-eslint": "^10.1.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-flowtype": "^5.9.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "prettier": "^2.6.1",
    "pretty-quick": "^3.1.3",
