{
  "name": "safe-airdrop",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --watchAll=false --verbose",
    "lint": "eslint --max-warnings 0 .",
    "coverage": "yarn test --coverage",
    "eject": "react-scripts eject",
    "fmt": "prettier --check '**/*.ts'",
    "fmt:write": "prettier --write '**/*.ts'",
    "prepare": "husky install",
    "generate-types": "typechain --target=ethers-v5 --out-dir src/contracts './node_modules/@openzeppelin/contracts/build/contracts/ERC20.json'",
    "postinstall": "yarn generate-types"
  },
  "dependencies": {
    "@fast-csv/parse": "^4.3.6",
    "@gnosis.pm/safe-apps-provider": "^0.9.0",
    "@gnosis.pm/safe-apps-react-sdk": "^4.0.5",
    "@gnosis.pm/safe-apps-sdk": "^6.0.0",
    "@gnosis.pm/safe-react-components": "^0.8.6",
    "@material-ui/core": "^4.12.3",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@openzeppelin/contracts": "^4.3.2",
    "ace-builds": "^1.4.13",
    "bignumber.js": "^9.0.1",
    "ethers": "^5.5.1",
    "honeyswap-default-token-list": "^3.1.1",
    "lodash.debounce": "^4.0.8",
    "react": "^17.0.2",
    "react-ace": "^9.5.0",
    "react-app-rewired": "^2.1.6",
    "react-csv-reader": "^3.4.0",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.4.2",
    "react-scripts": "^4.0.3",
    "react-svg": "^14.1.1",
    "styled-components": "^5.3.3",
    "tslib": "^2.3.1",
    "typescript": "~4.4.4"
  },
  "devDependencies": {
    "@simbathesailor/use-what-changed": "^2.0.0",
    "@testing-library/jest-dom": "^5.15.0",
    "@typechain/ethers-v5": "^7.1.2",
    "@types/chai": "^4.2.22",
    "@types/chai-as-promised": "^7.1.4",
    "@types/lodash.debounce": "^4.0.6",
    "@types/node": "^16.11.7",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@types/styled-components": "^5.1.15",
    "babel-eslint": "^10.1.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.9.2",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.4",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "typechain": "^5.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "./"
}
